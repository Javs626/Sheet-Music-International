<!DOCTYPE html>
<html lang="en">
<head>
<style type="text/css">
  body {
      background: transparent url(http://www.mutopiaproject.org/images/bg-darker.jpg) repeat scroll 30% -15%;
  background-repeat: repeat;
}
  </style>
</head>
<body>
      <% include ../views/partials/nav %>



  <div>
  <div>
    <% files.forEach((file) => {%>
        <% if(file.filename.charAt(0).toLowerCase() == 'c' || file.filename.charAt(0).toLowerCase() == 'd' ||
        file.filename.charAt(0).toLowerCase() == 'e') { console.log(file.filename) %>
 <a href="/file/<%= file._id %>" class="list-group-item">
      <h4 class="list-group-item-heading"><%= file.filename.replace(/\.[^/.]+$/, "") %></h4>
      <p class="list-group-item-text"><%= file.id %></p>
        <% } %>
   <%  }) %>
   </a>
    </div>
  </div>
  <footer>
